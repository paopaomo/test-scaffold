{
  "name": "react-test-scaffold",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html -p 8000",
    "cypress:open": "cypress open",
    "cypress": "yarn start & yarn cypress:open",
    "test": "jest --coverage",
    "ratchet": "jest-coverage-ratchet",
    "lint": "eslint --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "src/**": [
      "prettier --write --ignore-unknown",
      "yarn lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn ratchet",
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "coverageReporters": [
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100,
        "statements": 100,
        "functions": 100,
        "branches": 100
      }
    }
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/react": "^11.0.4",
    "babel-jest": "^26.3.0",
    "cypress": "^5.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-coverage-ratchet": "^0.2.3",
    "lint-staged": "^10.4.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2"
  }
}
