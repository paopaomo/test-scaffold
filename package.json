    "@testing-library/react": "^11.0.4",
    "babel-jest": "^26.3.0",
    "cypress": "^5.2.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-coverage-ratchet": "^0.2.3",
    "parcel-bundler": "^1.12.4"
  }
